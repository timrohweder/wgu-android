<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.rohwedercoursescheduler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AddEditMentorActivity"
            android:label="@string/title_activity_add_edit_mentor"
            android:parentActivityName=".MentorsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.rohwedercoursescheduler.MentorsActivity" />
        </activity>
        <activity
            android:name=".MentorDetailViewActivity"
            android:label="@string/title_activity_mentor_detail_view"
            android:parentActivityName=".MentorsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.rohwedercoursescheduler.MentorsActivity" />
        </activity>
        <activity
            android:name=".MentorsActivity"
            android:label="@string/title_activity_mentors"
            android:theme="@style/AppTheme.NoActionBar" />

        <receiver
            android:name=".AlarmReceiver"
            android:enabled="true"
            android:exported="false" />

        <activity
            android:name=".AssessmentDetailViewActivity"
            android:label="@string/title_activity_assessment_detail_view"
            android:parentActivityName=".AssessmentsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.rohwedercoursescheduler.AssessmentsActivity" />
        </activity>
        <activity
            android:name=".AddEditAssessmentActivity"
            android:label="@string/title_activity_add_edit_assessment"
            android:parentActivityName=".AssessmentsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.rohwedercoursescheduler.AssessmentsActivity" />
        </activity>
        <activity
            android:name=".CourseDetailViewActivity"
            android:label="@string/title_activity_course_detail_view"
            android:parentActivityName=".CoursesActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.rohwedercoursescheduler.CoursesActivity" />
        </activity>
        <activity
            android:name=".TermDetailViewActivity"
            android:label="@string/title_activity_term_detail_view"
            android:parentActivityName=".TermsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.rohwedercoursescheduler.TermsActivity" />
        </activity>
        <activity
            android:name=".AddEditCourseActivity"
            android:label="@string/title_activity_add_edit_course"
            android:parentActivityName=".CoursesActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.rohwedercoursescheduler.CoursesActivity" />
        </activity>
        <activity
            android:name=".AddEditTermActivity"
            android:label="@string/title_activity_add_term"
            android:parentActivityName=".TermsActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.rohwedercoursescheduler.TermsActivity" />
        </activity>
        <activity
            android:name=".AssessmentsActivity"
            android:label="@string/title_activity_assessments"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.rohwedercoursescheduler.MainActivity" />
        </activity>
        <activity
            android:name=".CoursesActivity"
            android:label="@string/title_activity_courses"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.rohwedercoursescheduler.MainActivity" />
        </activity>
        <activity
            android:name=".TermsActivity"
            android:label="@string/title_activity_terms"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.example.rohwedercoursescheduler.MainActivity" />
        </activity>
        <activity
            android:name=".NavigationActivity"
            android:label="@string/title_activity_navigation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>